{
    "name": "tidal/behat-console",
    "description": "A behat extension to provide contexts for testing console commands",
    "keywords": ["behat", "extension", "console", "symfony-console", "command", "application", "test", "bdd", "php7"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Timo Michna",
            "email": "timomichna@yahoo.de"
        }
    ],
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require": {
        "behat/behat": "^3.3",
        "symfony/console": "^3.2",
        "symfony/expression-language": "^3.2"
    },
    "require-dev": {
        "atoum/atoum": "^3.0",
        "atoum/stubs": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Tidal\\Behat\\ConsoleExtension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tidal\\Behat\\ConsoleExtension\\tests\\units\\": "tests/unit"
        }
    }
}
